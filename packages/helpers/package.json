{
	"name": "@jbootcaba/helpers",
	"version": "1.0.0",
	"main": "src/index.ts",
	"types": "src/index.ts",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"default": "./src/index.ts"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./src/index.ts"
			]
		}
	},
	"files": [
		"/dist"
	],
	"scripts": {
		"build:declaration": "tsc --emitDeclarationOnly",
		"build:clean": "rimraf dist/*",
		"build:esbuild": "ts-node ./build.ts",
		"build": "npm-run-all build:clean build:esbuild build:declaration"
	},
	"dependencies": {
		"@jbootcaba/http": "*",
		"@jbootcaba/inversify": "*",
		"@jbootcaba/resiliency": "*",
		"inversify": "^6.0.1",
		"type-fest": "^3.1.0"
	}
}
