{
	"name": "jbootcaba",
	"version": "1.0.0",
	"main": "src/index.ts",
	"types": "src/index.ts",
	"license": "MIT",
	"exports": {
		"./*": {
			"default": {
				"require": "./dist/index.js",
				"default": "./dist/index.d.ts"
			}
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./src/index.ts"
			]
		}
	},
	"files": [
		"/dist"
	],
	"scripts": {
		"build:declaration": "tsc --emitDeclarationOnly",
		"build:clean": "rimraf dist/*",
		"build:esbuild": "ts-node ./build.ts",
		"build": "npm-run-all build:clean build:esbuild build:declaration"
	},
	"dependencies": {
		"@jbootcaba/core": "*",
		"@jbootcaba/config": "*",
		"@jbootcaba/express": "*",
		"@jbootcaba/helpers": "*",
		"@jbootcaba/resiliency": "*",
		"@jbootcaba/logger-pino": "*",
		"@jbootcaba/logger": "*",
		"@jbootcaba/inversify": "*",
		"@jbootcaba/http": "*"
	},
	"devDependencies": {
		"esbuild-node-externals": "^1.5.0"
	}
}
