{
	"name": "jbootcaba",
	"version": "1.0.0",
	"main": "src/index.ts",
	"types": "src/index.ts",
	"license": "MIT",
	"exports": {
		"./*": {
			"default": {
				"require": "./dist/index.js",
				"default": "./src/index.ts"
			}
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./src/index.ts"
			]
		}
	},
	"files": [
		"/dist"
	],
	"scripts": {
		"build:declaration": "tsc --emitDeclarationOnly",
		"build:clean": "rimraf dist/*",
		"build:esbuild": "ts-node ./build.ts",
		"build": "npm-run-all build:clean build:esbuild"
	},
	"dependencies": {},
	"devDependencies": {
		"esbuild-node-externals": "^1.5.0"
	}
}
